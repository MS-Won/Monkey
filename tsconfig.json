  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./frontend",
    "paths": {
      "@env": ["./frontend/env.d.ts"]
    }
  },
  "include": ["frontend/**/*"]
}
